{
  "mcpServers": {
    "browsertools": {
      "command": "npx",
      "args": [
        "-y",
        "@agentdeskai/browser-tools-mcp"
      ]
    },
    "wezterm-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "wezterm-mcp"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/joshuamcnabb/Development/sol-intelligence"
      ]
    },
    "puppeteer": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--init",
        "--network",
        "host",
        "-e",
        "DOCKER_CONTAINER=true",
        "mcp/puppeteer"
      ]
    },
    "Sentry": {
      "url": "https://mcp.sentry.dev/mcp"
    },
    "context7": {
      "url": "https://mcp.context7.com/mcp"
    },
    "next-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "next-devtools-mcp@latest"
      ]
    }
  }
}